<?xml version="1.0" encoding="UTF-8"?>
<template id="ethiopian_calander_template" xml:space="preserve">
    <t t-name='ethiopian_calander_widget'>
        <div class="o_datepicker">
            <input type="text"
                class="o_datepicker_input o_input oe_simple_date oe_datepicker_master"
                t-att-name="widget.name"
                
                id="popupDatepicker"
                t-att-placeholder="placeholder"/>
            <span class="o_datepicker_button"/>
        </div>
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/EthiopianCalendar/static/js/custom.js"></script>
        </xpath>
    </t>
