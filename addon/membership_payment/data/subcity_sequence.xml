<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        

        <!-- Sequence for Service Order. -->
        <record id="sequence_woreda_order" model="ir.sequence">
           <field name="name">Woreda Sequence</field>
           <field name="code">Woreda.payment</field>
           <field name="prefix">Woreda/%(year)s/</field>
           <field name="padding">5</field>
           <field name="implementation">no_gap</field>
        </record>
        <record id="sequence_subcity_order" model="ir.sequence">
           <field name="name">Sub city Sequence</field>
           <field name="code">sub.city.payment</field>
           <field name="prefix">sub-city/%(year)s/</field>
           <field name="padding">5</field>
           <field name="implementation">no_gap</field>
        </record>
        <record id="sequence_city_order" model="ir.sequence">
           <field name="name">city Sequence</field>
           <field name="code">city.payment</field>
           <field name="prefix">AAPP%(year)s/</field>
           <field name="padding">5</field>
           <field name="implementation">no_gap</field>
        </record>
    </data>
</odoo>
